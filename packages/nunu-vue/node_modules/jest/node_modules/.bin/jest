#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules/jest/bin/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules/jest/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/node_modules:/mnt/c/Users/LIYONG/Desktop/node_modules:/mnt/c/Users/LIYONG/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules/jest/bin/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules/jest/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/jest@27.5.1_supports-color@7.2.0/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules/.pnpm/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/nunu-xxx/node_modules:/mnt/c/Users/LIYONG/Desktop/my-lib/node_modules:/mnt/c/Users/LIYONG/Desktop/node_modules:/mnt/c/Users/LIYONG/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
